{
  "name": "bibbox",
  "description": "Library box - automatic borrower",
  "version": "1.4.2",
  "private": true,
  "dependencies": {
    "architect": "^0.1.13",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "scripts": {
    "run": "node app.js",
    "check-coding-standards-eslint": "eslint --config .eslintrc.js '**/*.js'",
    "check-coding-standards-js": "yarn run check-coding-standards-eslint",
    "check-coding-standards": "yarn run check-coding-standards-js",
    "apply-coding-standards-eslint": "eslint --config .eslintrc.js '**/*.js' --fix",
    "apply-coding-standards-js": "yarn run apply-coding-standards-eslint",
    "apply-coding-standards": "yarn run apply-coding-standards-js"
  }
}
