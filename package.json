{
    "name": "frontend",
    "version": "1.0.0",
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
        "@babel/plugin-proposal-optional-chaining": "^7.18.9",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@formatjs/cli": "^2.15.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-regular-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.19",
        "@svgr/webpack": "^6.5.1",
        "@symfony/webpack-encore": "^4.2.0",
        "babel-plugin-react-intl": "^8.2.25",
        "babel-preset-react-app": "^9.1.2",
        "bootstrap": "^4.6.2",
        "core-js": "^3.26.1",
        "css-loader": "^3.6.0",
        "eslint": "^8.0",
        "eslint-config-standard": "^17.0.0",
        "cypress": "^12.7.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-standard": "^5.0.0",
        "postcss": "^8.4.20",
        "postcss-loader": "^7.0.2",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-bootstrap": "^1.6.6",
        "react-dev-utils": "^12.0.1",
        "react-dom": "^18.2.0",
        "react-idle-timer": "^4.6.4",
        "react-intl": "^5.25.1",
        "react-simple-keyboard": "^3.5.30",
        "react-test-renderer": "^18.2.0",
        "regenerator-runtime": "^0.13.11",
        "sass": "^1.56.2",
        "sass-loader": "^13.2.0",
        "socket.io-client": "^4.6.1",
        "style-loader": "^1.3.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-notifier": "^1.15.0"
    },
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "dev-server": "encore dev-server",
        "dev": "encore dev",
        "watch": "encore dev --watch",
        "build": "encore production --progress",
        "extract": "formatjs extract",
        "compile": "formatjs compile",
        "cypress": "cypress open",
        "check-coding-standards": "eslint --config .eslintrc.js 'assets/**/*.js",
        "apply-coding-standards": "eslint --config .eslintrc.js 'assets/**/*.js' --fix"
    },
    "engines": {
        "node": "=18.*"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "roots": [
            "<rootDir>/assets/js"
        ],
        "collectCoverageFrom": [
            "assets/js/**/*.{js,jsx,ts,tsx}",
            "!assets/js/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/assets/js/setupTests.js"
        ],
        "testMatch": [
            "<rootDir>/assets/js/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/assets/js/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/assets/js/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/assets/js/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    }
}
